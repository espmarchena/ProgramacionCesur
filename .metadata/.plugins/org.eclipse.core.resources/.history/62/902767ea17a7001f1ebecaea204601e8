		int tabla[][] = new int [10][10];
				
		int nAleatorio= 0;
		  
		    	
		for (int i=0; i<tabla.length; i++) {	               //PASO 1
			int max=10;
			int min=0;
			int range= (max-min) +1;
			    		
			nAleatorio = (int) ((range * Math.random()) +min);
			
			for (int j=0; j<tabla.length; j++) {
			
				nAleatorio = tabla[i][j];
				System.out.println(tabla[i][j]);
			}
			
		}
				
	}

}
